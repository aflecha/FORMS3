			This.Caption = "REIMPRESIÓN DE TICKETS"
			This.lblComprob.Visible  = .f.   
			This.CboTipoComp.Visible = .f. 
			This.OptTipo.Visible     = .f. 
			This.txtnCopias.Value    = 1  
		
		ELSE
		
			This.lblCopias.Visible   = .f. 
			This.txtnCopias.Visible  = .f.   	 	    
		
		ENDIF   
		
	ENDPROC

