	PROCEDURE Init
		LPARAMETERS tlEsTicket
		
		Thisform.lEsTicket = tlEsTicket
		
		oapp.Setear_Objetos( This, .T. )
		
		thisform.txtFechaDes.Value = DATE()
		thisform.txtFechaHas.Value = DATE()
		
